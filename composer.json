{
    "name": "mautic/omnivery-mailer-bundle",
    "description": "Integrate Swiftmailer transport for Omnivery API",
    "type": "mautic-plugin",
    "keywords": [
        "mautic",
        "plugin",
        "integration",
        "omnivery"
    ],
    "extra": {
        "install-directory-name": "OmniveryMailerBundle"
    },
    "scripts": {
        "lint": "vendor/bin/parallel-lint --exclude ./vendor .",
        "checkcs": "php-cs-fixer fix --config=./.php_cs.dist.php --diff --dry-run",
        "fixcs": "php-cs-fixer fix --config=./.php_cs.dist.php"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.0 <8.1",
        "mautic/core-lib": "^4.0"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "friendsofphp/php-cs-fixer": "2.19.0"
    }
}
